<% form_for :dump, :url=>{ :action	=>	"import_from_csv"},
                    		:html 	=> 	{ 	:multipart => true } do |f| -%>
<table>
	<tr>
		<td>
      		<label for="dump_file">
        		Select a CSV File :
      		</label>
     	</td>
     	<td >
       		<%= f.file_field :file -%>
     	</td>
   	</tr>
   	<tr>
     	<td colspan='2'>
       		<%= submit_tag 'Submit' -%>
     	</td>
   	</tr>
</table>
<% end -%>