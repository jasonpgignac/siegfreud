<h1>Listing actions</h1>

<table>
  <tr>
  </tr>

<% @actions.each do |action| %>
  <tr>
	<td><%= action.created_at.to_s%></td>
	<td><%= action.title %></td>
	<td><%= action.description %></td>
	<td>
		<% action.action_inventory_objects.each do |aio| %>
			<%= aio.inventory_object.short_name %><br/>
		<% end %>
	</td>
  </tr>
<% end %>
</table>

<br />